{
  "name": "prevent-parent-scroll",
  "version": "0.0.1",
  "main": "dist/index.js",
  "description": "Prevent scroll of an HTML Element from bubbling to its parents",
  "keywords": [
    "prevent",
    "parent",
    "scroll",
    "mousewheel"
  ],
  "scripts": {
    "preversion": "npm run test",
    "clean": "rm -rf dist",
    "precompile": "npm run clean",
    "compile": "node_modules/.bin/tsc -p .",
    "watch": "node_modules/.bin/watch 'npm run test' src",
    "prestart": "npm run test",
    "start": "node dist/index.js",
    "dev": "npm run watch",
    "pretest": "npm run compile",
    "test": "node_modules/.bin/tape 'dist/**/*.spec.js' | tap-spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aliatsis/prevent-parent-scroll.git"
  },
  "devDependencies": {
    "@types/es6-promise": "~0.0.32",
    "@types/mockery": "~1.4.29",
    "@types/node": "~6.0.45",
    "@types/sinon": "~1.16.31",
    "@types/sinon-as-promised": "~4.0.4",
    "@types/tape": "~4.2.28",
    "mockery": "~1.4.0",
    "sinon": "~1.17.2",
    "sinon-as-promised": "~4.0.0",
    "tap-spec": "~4.1.1",
    "tape": "~4.5.1",
    "tape-promise": "~1.0.1",
    "tslint": "~3.15.0",
    "typescript": "~2.0.0",
    "watch": "~0.17.1"
  },
  "engines": {
    "node": "4.3.x",
    "npm": "3.8.x"
  },
  "typings": "dist/index.d.ts"
}
